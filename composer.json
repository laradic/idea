{
    "name": "laradic/idea",
    "description": "Laravel 5 IntelliJ IDEA / PHPStorm helpers and improvements like Advanced Metadata generators (autocompletion), fixing settings, better code completion, improved package development, etc",
    "keywords": ["laravel", "laradic", "intellij", "idea", "phpstorm", "autocomplete", "repositories", "structure"],
    "homepage": "https://radic.io",
    "license": "MIT",
    "authors": [
        {"name": "Robin Radic", "homepage": "https://radic.io", "email": "rradic@hotmail.com", "role": "Developer"}
    ],
    "require": {
        "php": ">=7.1.0",
        "illuminate/support": "~5.0|~6.0",
        "barryvdh/laravel-ide-helper": "^2.6",
        "laradic/support": "^2.0.0",
        "laradic/generators": "^2.0.0",
        "openlss/lib-array2xml": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Laradic\\Idea\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laradic\\Tests\\Idea\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "2.1.x-dev"
        },
        "laravel": {
        }
    }
}
